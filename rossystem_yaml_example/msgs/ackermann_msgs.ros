ackermann_msgs:
  specs:
    msg: AckermannDrive
      message:
		float32 speed                   # desired forward speed (m/s)
		float32 acceleration            # desired acceleration (m/s^2)
		float32 jerk                    # desired jerk (m/s^3)
    msg: AckermannDriveStamped
      message:
		Header          header
		AckermannDrive  drive